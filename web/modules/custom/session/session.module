<?php

/**
 * @file
 * Contains session.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function session_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the session module.
    case 'help.page.session':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Session content type') . '</p>';
      return $output;

    default:
  }
}
